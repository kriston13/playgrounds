<h1>Tell the world about a playground</h1>

<%= form_for @playground do |f| %>


    <%= f.label :name %>
    <%= f.text_field :name %><br />
    
    <%= f.label :suburb %>
    <%= f.text_field :suburb %><br />
    
    <%= f.label :state %>
    <%= f.text_field :state %><br />
    <%= f.label :country %>
    <%= f.text_field :country %><br />

    <%= f.hidden_field :latitude %>
    <%= f.hidden_field :longitude %>
    
    <%= f.submit "Submit"%>
    
<% end %>
